{
  "name": "@adashino/core",
  "version": "1.0.0",
  "description": "Core for running the framework to create bots for discorc with @biscuitland/core.Core for running the framework to create bots for discorc with @biscuitland/core.",
  "main": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "author": "@kaname-png",
  "license": "MIT",
  "private": true,
  "type": "module",
  "repository": "https://github.com/kaname-png/adashino/tree/main/packages/core",
  "homepage": "https://github.com/kaname-png/adashino",
  "keywords": [
    "framework",
    "biscuitland",
    "discord",
    "bot"
  ],
  "packemon": [
    {
      "format": "lib",
      "platform": "node",
      "support": "experimental"
    },
    {
      "format": "mjs",
      "platform": "node",
      "support": "experimental"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.5.0"
  },
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "dependencies": {
    "@adashino/structures": "workspace:^"
  },
  "exports": {
    "import": "./mjs/index.mjs",
    "require": "./lib/index.js",
    "types": "./dts/index.d.ts"
  }
}
