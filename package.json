		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"git-cz": "^4.9.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
