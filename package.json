{
	"name": "main",
	"description": "A @biscuitland/core framework?",
	"type": "module",
	"private": true,
	"sideEffects": false,
	"author": "@kaname-png",
	"license": "MIT",
	"scripts": {
		"build": "moon run :build",
		"clean": "moon clean && rimraf \"packages/**/dist\"",
		"generate": "moon generate",
		"commit": "git-cz",
		"format": "prettier --write packages/**/*.{tsx,ts,css} --no-error-on-unmatched-pattern",
		"lint": "moon run :lint",
		"moon": "moon",
		"prepare": "husky install .husky",
		"prettier": "moon run :prettier",
		"update:f": "pnpm up --latest",
		"update:i": "pnpm up --interactive"
	},
	"keywords": [
		"discord",
		"biscuit",
		"framework discord",
		"bot"
	],
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@commitlint/format": "^17.0.0",
		"@moonrepo/cli": "^0.16.1",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^18.8.4",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"git-cz": "^4.9.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"rimraf": "^3.0.2",
		"tslib": "^2.4.0",
		"typescript": "^4.8.4"
	},
	"engines": {
		"node": "18.10.0"
	},
	"packageManager": "pnpm@7.12.1"
}
